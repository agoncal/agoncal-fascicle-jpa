CREATE TABLE TRACK (ID BIGINT NOT NULL, DESCRIPTION VARCHAR, DURATION DOUBLE, TITLE VARCHAR, WAV LONGVARBINARY, PRIMARY KEY (ID))
CREATE TABLE BOOK (ID BIGINT NOT NULL, DESCRIPTION VARCHAR(2000), ILLUSTRATIONS BOOLEAN, ISBN VARCHAR, nb_of_pages INTEGER NOT NULL, PRICE DOUBLE, book_title VARCHAR NOT NULL, PRIMARY KEY (ID))
CREATE TABLE NEWS (LANGUAGE VARCHAR NOT NULL, TITLE VARCHAR NOT NULL, CONTENT VARCHAR, PRIMARY KEY (LANGUAGE, TITLE))
CREATE TABLE CUSTOMER (ID BIGINT NOT NULL, CREATIONDATE TIMESTAMP, DATEOFBIRTH DATE, EMAIL VARCHAR, FIRSTNAME VARCHAR, LASTNAME VARCHAR, PHONENUMBER VARCHAR, PRIMARY KEY (ID))
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT NUMERIC(38), PRIMARY KEY (SEQ_NAME))

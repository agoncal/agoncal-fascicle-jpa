CREATE TABLE t_book (ID BIGINT NOT NULL, DESCRIPTION VARCHAR, ILLUSTRATIONS BOOLEAN, ISBN VARCHAR, NBOFPAGES INTEGER, PRICE DOUBLE, TITLE VARCHAR, PRIMARY KEY (ID))
CREATE TABLE ADDRESS (ID BIGINT NOT NULL, STREET1 VARCHAR, STREET2 VARCHAR, PRIMARY KEY (ID))
CREATE TABLE t_city (ID BIGINT NOT NULL, CITY VARCHAR, STATE VARCHAR, ZIPCODE VARCHAR, PRIMARY KEY (ID))
CREATE TABLE t_country (ID BIGINT NOT NULL, COUNTRY VARCHAR, PRIMARY KEY (ID))
ALTER TABLE t_city ADD CONSTRAINT FK_t_city_ID FOREIGN KEY (ID) REFERENCES ADDRESS (ID)
ALTER TABLE t_country ADD CONSTRAINT FK_t_country_ID FOREIGN KEY (ID) REFERENCES ADDRESS (ID)
